<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('partials/head'); -%>
    <link rel="stylesheet" href="/jobseeker/jobseeker.css">
    <title>Document</title>
</head>

<body>
    <%- include('partials/navbar-jobseeker'); -%>

    <section id="options">
        <div class="option suggest active-option" data-target="section#suggest-job">
            <i class="far fa-thumbs-up"></i>
            Việc phù hợp
        </div>
        <div class="option pending"  data-target="section#pending-job">
            <i class="fas fa-marker"></i>
            Việc ứng tuyển
        </div>
        <div class="option love-option">
            <i class="fas fa-heart"></i>
            Việc yêu thích
        </div>
    </section>
    <section id="suggest-job">
    </section>
    <section id="pending-job" class="<%=user.id%>">

    </section>
    <%- include('partials/scripts'); -%>
    <!-- JS -->
    <script src="/jobseeker/jobseeker.js"></script>
</body>

</html>